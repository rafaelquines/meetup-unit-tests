    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.2",
    "@types/superagent": "^4.1.3",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "mocha": "^10.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb-base": "^0.3.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^3.5.3"
