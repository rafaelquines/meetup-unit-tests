  "nyc": {
    "check-coverage": false,
    "exclude": [
      "**/*.js",
      "test",
      "build",
      "dist"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "extension": [
      ".ts"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
